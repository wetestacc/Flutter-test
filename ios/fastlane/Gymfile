export_options(
  {
    method: "app-store",
    signingStyle: "manual",
    manageAppVersionAndBuildNumber: false,
    uploadSymbols: true,
    silent: true,
    provisioningProfiles: { 
      "com.example.fluttertest" => "match AppStore com.example.fluttertest",
    },
  },
)